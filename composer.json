{
    "name": "dedomorozoff/max-messenger-php",
    "description": "PHP library for MAX Messenger API integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dedo Morozoff",
            "email": "dedomorozoff@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "MaxMessenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaxMessenger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MaxMessenger\\MaxMessengerServiceProvider"
            ],
            "aliases": {
                "MaxMessenger": "MaxMessenger\\Facades\\MaxMessenger"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

